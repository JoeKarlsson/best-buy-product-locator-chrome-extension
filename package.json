{
  "name": "best-buy-product-locator-chrome-extension",
  "version": "0.9.0",
  "description": "Two day shipping to slow for you? This chrome extension will tell you if the Amazon product you are browsing is available at a Best Buy store for immediate in store pickup.",
  "scripts": {
    "start": "node scripts/dev",
    "dev": "node scripts/dev",
    "test": "jest --coverage",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint app chrome scripts webpack/*.js",
    "healthcheck": "npm run lint && npm run test",
    "prepublish": "npm run build",
    "precommit": "npm run lint",
    "prepush": "npm run healthcheck",
    "postcommit": "git status && pretty-quick --staged",
    "postmerge": "npm install && npm audit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeKarlsson/best-buy-product-locator-chrome-extension"
  },
  "bugs": {
    "url": "https://github.com/JoeKarlsson/best-buy-product-locator-chrome-extension/issues"
  },
  "keywords": [
    "Best Buy",
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Joe Karlsson <joekarlsson1@gmail.com> & Eric Moore <moore.ericc@gmail.com >",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chromedriver": "^2.33.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "crx": "^3.0.3",
    "dotenv": "^6.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "^6.5.2",
    "file-loader": "^2.0.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.5.0",
    "jest-webextension-mock": "^3.4.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "minimist": "^1.2.0",
    "nyc": "^12.0.2",
    "postcss-loader": "^1.2.1",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "pug-cli": "^1.0.0-alpha6",
    "react-addons-test-utils": "^15.0.2",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.4.3",
    "shelljs": "^0.8.2",
    "webpack": "^3.10.0",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-httpolyglot-server": "^0.3.0"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.4.5",
    "webfontloader": "^1.6.28"
  }
}
